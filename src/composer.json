{
  "name": "fabd/koohii",
  "type": "project",
  "description": "Kanji Koohii web app",
  "require": {
    "erusev/parsedown": "1.7.4",
    "erusev/parsedown-extra": "0.8.1",
    "illuminate/collections": "^8.83",
    "symfony/var-dumper": "^5.4",
    "phpmailer/phpmailer": "^6.8",
    "friendsofsymfony1/symfony1": "1.5.*",
    "php-http/guzzle7-adapter": "^1.1",
    "nyholm/psr7": "^1.8",
    "mailersend/mailersend": "^0.35.0"
  },
  "autoload": {
    "psr-4": {
      "Koohii\\": "lib/",
      "": "lib/peer/"
    },
    "exclude-from-classmap": [
      "vendor/erusev/parsedown-extra/test/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Koohii\\Scripts\\": "data/scripts/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
