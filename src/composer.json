{
  "name": "fabd/koohii",
  "type": "project",
  "description": "Kanji Koohii web app",
  "require": {
    "erusev/parsedown": "1.7.4",
    "erusev/parsedown-extra": "0.8.1",
    "illuminate/collections": "^8.83",
    "symfony/var-dumper": "^5.4",
    "phpmailer/phpmailer": "^6.8"
  },
  "autoload": {
    "psr-4": {
      "Koohii\\": "lib/"
    },
    "exclude-from-classmap": [
      "vendor/erusev/parsedown-extra/test/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Koohii\\Scripts\\": "data/scripts/"
    }
  }
}
